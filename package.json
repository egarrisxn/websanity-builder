    "@sanity/vision": "^3.87.0",
    "classnames": "^2.5.1",
    "next": "^15.3.1",
    "next-sanity": "^9.10.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "sanity": "^3.87.0",
    "server-only": "0.0.1",
    "styled-components": "^6.1.17"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
