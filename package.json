{
  "name": "websanity-builder",
  "description": "This is your next website builder!",
  "version": "1.0.0",
  "homepage": "https://websanitybuilder.vercel.app/",
  "author": "Ethan G. (https://egxworld.net)",
  "license": "MIT",
  "keywords": [
    "sanity",
    "nextjs",
    "react",
    "tailwindcss",
    "vercel"
  ],
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format": "prettier --check --ignore-path .gitignore .",
    "lint": "next lint .",
    "lint:fix": "pnpm run format && pnpm run lint -- --fix"
  },
  "dependencies": {
    "@sanity/icons": "^3.4.0",
    "@sanity/image-url": "^1.1.0",
    "@sanity/preview-url-secret": "^2.0.4",
    "@sanity/react-loader": "^1.10.23",
    "@sanity/ui": "^2.8.26",
    "@sanity/vision": "^3.65.1",
    "classnames": "^2.5.1",
    "next": "^14.2.4",
    "next-sanity": "^9.8.19",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sanity": "^3.65.1",
    "server-only": "0.0.1",
    "styled-components": "^6.1.13"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.15",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.4",
    "eslint-config-prettier": "^9.1.0",
    "postcss": "^8.4.49",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "^3.4.15"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "useTabs": false,
    "tabWidth": 2,
    "plugins": [
      "prettier-plugin-tailwindcss"
    ]
  }
}
